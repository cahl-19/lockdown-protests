<!DOCTYPE html>
<html lang="{{lang}}">
{{> content }}
<head>
    <meta charset="{{charset}}">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=yes">
    {{#each metaProperty as |meta|}}<meta property="{{meta.property}}" content="{{meta.content}}">
    {{/each}}
    <title>{{title}}</title>
    {{#each scripts as |script|}}<script src="{{script}}"></script>
    {{/each}}
    {{#each inlineScript as |script|}}<script>{{{script}}}</script>
    {{/each}}
    {{#> pageTemplateScripts }}{{/pageTemplateScripts}}
    {{#each styleSheets as |s|}}<link rel="stylesheet" href="{{s}}">
    {{/each}}
    {{#> pageTemplateStyleSheets }}{{/pageTemplateStyleSheets}}
</head>

<body>
    {{#> body }}{{/body}}
</body>

</html>
